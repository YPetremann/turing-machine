{
  "name": "turing-machine",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "check": "bun run check:biome && bun run check:unused",
    "check:biome": "biome check .",
    "check:unused": "knip",
    "fix:biome": "biome check . --fix",
    "fix:unused": "knip --fix",
    "test": "vitest",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@iconify/json": "^2.2.346",
    "@iconify/tailwind4": "^1.0.6",
    "@tailwindcss/vite": "^4.1.8",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "iconify-icon": "^3.0.0",
    "knip": "^5.61.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
